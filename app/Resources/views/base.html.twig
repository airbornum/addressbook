<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
            {% stylesheets filter='cssrewrite'
                    '%kernel.root_dir%/../web/css/bootstrap.css'
                    '%kernel.root_dir%/../web/css/font-awesome.css'
                    '%kernel.root_dir%/../web/css/style.css'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="screen" />
        {% endstylesheets %}
    </head>
    <body>
        {% block body %}
            <section class="for-full-back color-white " id="contact">
                <div class="container">
                    {% block content %}
                    {% endblock %}
                </div>
            </section>
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
